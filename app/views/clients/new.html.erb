<%= form_for(@client, url: studio_clients_path) do |f| %>
<%= f.label :name %>
<%= f.text_field :name%></br>
<%= f.label :email %>
<%= f.text_field :email%></br>
<%= f.label :genre %>
<%= f.text_field :genre%></br>
<%= f.label :budget %>
<%= f.text_field :budget%></br>
<%= f.label :band %>
<%= f.check_box :band%></br>
<%= f.label :drums %>
<%= f.check_box :drums %></br>
Session Information: </br>
<%= f.fields_for :recording_sessions do |session_form| %>
  <%= session_form.label :start_date %>
  <%= session_form.date_field :start_date %></br>
  <%= session_form.label :end_date %>
  <%= session_form.date_field :end_date%></br>
  <%= session_form.label :hours_per_day %>
  <%= session_form.text_field :hours_per_day%></br>
  <%= session_form.label :per_hour %>
  <%= session_form.text_field :per_hour %></br>
<% end %> 
<%= f.submit %>
<% end %>
