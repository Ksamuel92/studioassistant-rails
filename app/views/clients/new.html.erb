<%= form_for([@studio, @client]) do |f| %>
<%= f.label :name %>
<%= f.text_field :name%></br>
<%= f.label :email %>
<%= f.text_field :email%></br>
<%= f.label :genre %>
<%= f.text_field :genre%></br>
<%= f.label :budget %>
<%= f.text_field :budget%></br>
<%= f.label :band %>
<%= f.check_box :band%></br>
<%= f.label :drums %>
<%= f.check_box :drums %></br>
Session Information: </br>
<%= f.fields_for @recordingsession do |session_field| %>
  <%= session_field.label :start_date %>
  <%= session_field.date_field :start_date %></br>
  <%= session_field.label :end_date %>
  <%= session_field.date_field :end_date%></br>
  <%= session_field.label :hours_per_day %>
  <%= session_field.text_field :hours_per_day%></br>
  <%= session_field.label :per_hour %>
  <%= session_field.text_field :per_hour %></br>
<% end %> 
<%= f.submit %>
<% end %>
