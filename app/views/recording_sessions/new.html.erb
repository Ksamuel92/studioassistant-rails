<%= form_for([@studio, @recordingsession]) do |f| %>
<%= f.hidden_field :studio_id, value: @studio.id %>
<%= f.label :start_date %>
<%= f.date_field :start_date %></br>
<%= f.label :end_date %>
<%= f.date_field :end_date%></br>
<%= f.label :hours_per_day %>
<%= f.text_field :hours_per_day%></br>
<%= f.label :per_hour %>
<%= f.text_field :per_hour%></br>
    Client Information: </br>
<%= f.fields_for :client do |client_field| %>
  <%= client_field.label :name %>
  <%= client_field.text_field :name%></br>
  <%= client_field.label :email %>
  <%= client_field.text_field :email%></br>
  <%= client_field.label :genre %>
  <%= client_field.text_field :genre%></br>
  <%= client_field.label :budget %>
  <%= client_field.text_field :budget%></br>
  <%= client_field.label :band %>
  <%= client_field.check_box :band%></br>
  <%= client_field.label :drums %>
  <%= client_field.check_box :drums %></br>
<% end %> 
<%= f.submit %>
<% end %>
