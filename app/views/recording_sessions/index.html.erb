<%= index_header_sessions %>
<ul>
<% @recordingsessions.each do |session| %>
    <li> <%= link_to "Session at #{session.studio.name} with #{session.client.name} on #{session.start_date}", studio_recording_session_path(session.studio, session) %>
    <%= button_to 'Delete Session', recording_session_path(session), method: :delete, :onclick => "return confirm(Are you sure you want to delete this session?)" %>
<% end%>
</ul>    
