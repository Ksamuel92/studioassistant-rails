<ul>
<% @studios.each do |studio| %>
    <li> <%= link_to studio.name, studio_path(studio) %> - <%= studio.email %> </li>
    <li> <%= link_to "Create New Session at #{studio.name}", new_studio_recording_session_path(studio) %> </li>

<% studio.recording_sessions.each do |session| %>
        <li> <%= link_to "Session with #{session.client.name}", recording_session_path(session) %> </li> 
    <% end %>
<% end %>
</ul>

