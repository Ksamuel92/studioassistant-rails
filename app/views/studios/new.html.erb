<h3>Create New Studio√ccç</h3>

<%= form_with model: @studio do |f| %>
<%= f.label :studio_name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email%>
<%= f.label :address_line_1, "Address Line 1" %>
<%= f.text_field :address_line_1%>
<%= f.label :address_line_2, "Address Line 2" %>
<%= f.text_field :address_line_2%>
<%= f.label :city %>
<%= f.text_field :city%>
<%= f.label :state %>
<%= f.select :state, options_for_select(us_states), include_blank: true %>
<%= f.label :zip_code %>
<%= f.text_field :zip_code%>
<%= f.label :daw %>
<%= f.text_field :daw%>
<%= f.label :rental_cost %>
<%= f.number_field :rental_cost%>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.submit %>
<% end %>


<%= @test_message %>




