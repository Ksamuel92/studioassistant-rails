<%= form_with model: @studio do |f| %>
<%= f.label :studio_name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email%>
<%= f.label :address %>
<%= f.text_field :address%>
<%= f.label :daw %>
<%= f.text_field :daw%>
<%= f.label :rental_cost %>
<%= f.number_field :rental_cost%>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.submit %>
<% end %>


<%= @test_message %>




